    "@mantine/notifications": "^7.15.2",
    "@tabler/icons-react": "^3.27.1",
    "@types/node": "22.5.5",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "concurrently": "^9.0.1",
